{
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "http://file.malexple.ru/static/$1"
    },
    {
      "source": "/media/(.*)",
      "destination": "http://file.malexple.ru/media/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/proxy/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/proxy/$1"
    },
    {
      "source": "/favicon.png",
      "destination": "/favicon.png"
    },
    {
      "source": "/(.*)",
      "destination": "/api/proxy/$1"
    }
  ]
}